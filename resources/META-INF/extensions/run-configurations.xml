<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <configurationType implementation="nl.hannahsten.texifyidea.run.LatexRunConfigurationType"/>
        <configurationType implementation="nl.hannahsten.texifyidea.run.legacy.bibtex.BibtexRunConfigurationType"/>
        <configurationType implementation="nl.hannahsten.texifyidea.run.legacy.makeindex.MakeindexRunConfigurationType"/>
        <configurationType implementation="nl.hannahsten.texifyidea.run.legacy.externaltool.ExternalToolRunConfigurationType"/>
        <runConfigurationProducer implementation="nl.hannahsten.texifyidea.run.LatexRunConfigurationProducer"/>
        <projectService serviceImplementation="nl.hannahsten.texifyidea.service.LatexRunConfigurationViewManagerService"/>
        <pathMacroContributor implementation="nl.hannahsten.texifyidea.run.macro.LatexPathMacroContributor" />
        <macro implementation="nl.hannahsten.texifyidea.run.macro.MainFileDirMacro" />
        <macro implementation="nl.hannahsten.texifyidea.run.macro.OutputDirMacro" />
        <projectTaskRunner implementation="nl.hannahsten.texifyidea.run.LatexProjectTaskRunner" />
    </extensions>
</idea-plugin>